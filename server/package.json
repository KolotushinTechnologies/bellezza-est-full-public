  "description": "API server for flower shop",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "seed:admin": "ts-node src/utils/seed-admin.ts",
    "seed:services": "ts-node src/utils/seed-services.ts",
    "seed:portfolio": "ts-node src/utils/seed-portfolio.ts",
    "seed:care": "ts-node src/utils/seed-care.ts",
    "seed:blog": "ts-node src/utils/seed-blog.ts",
    "seed:all": "npm run seed:services && npm run seed:portfolio && npm run seed:care && npm run seed:blog",
    "proxy": "node cors-proxy.js",
    "simple-server": "node simple-cors-server.js",
    "start-all": "node start-servers.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.96",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "http-proxy-middleware": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "nodemon": "^3.1.10",
    "ts-node-dev": "^2.0.0"
  }
